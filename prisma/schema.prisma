
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}
model User {
  id        Int      @id @default(autoincrement())
  name      String
  verified  Boolean  @default(false)
  email     String   @unique
  club     String?
  status   String?
  statuscategory   String?
  afterSchool String?
  classNumber String?
  statusmoreInfo String?
}